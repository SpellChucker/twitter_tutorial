<h1>Tweets#index</h1>
<p>Find me in app/views/tweets/index.html.erb</p>

<%= form_for @tweet do |f| %>
  <%= f.text_field :body, :placeholder => 'Type your thoughts here' %>
  <%= f.submit %>
<% end %>

<% @tweets.each do |tweet| %>
  <%= render partial: 'tweet_box', locals: { tweet: tweet } %>
<% end %>
